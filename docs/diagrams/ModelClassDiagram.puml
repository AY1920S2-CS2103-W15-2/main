@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class Session
Class Model
Class ModelManager
Class UserPrefs
Class ReadOnlyUserPrefs

Package Interviewee {
Class Interviewee
Class Alias
Class ID
Class Name
Class CV
Class Transcript
Class UniqueIntervieweeList
}

Package Question {
Class Question
Class QuestionName
Class Number
Class Answer
Class UniqueQuestionList
}

Package Attribute {
Class Attribute
Class AttributeName
Class UniqueAttributeList
}

Package Metric {
Class UniqueMetricList
Class Metric
}

Package Transcript {
Class Transcript
Class RemarksMap
Class Audio
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o--> "1" Session
ModelManager o-left-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

Session *--> "1" UniqueIntervieweeList
UniqueIntervieweeList o--> "*" Interviewee
Interviewee *--> Name
Interviewee *--> Alias
Interviewee *--> ID
Interviewee *--> CV
Interviewee *--> "*" Transcript
Transcript *--> RemarksMap
Transcript *--> Audio

Session *--> "1" UniqueAttributeList
UniqueAttributeList o--> "*" Attribute
Attribute *--> AttributeName

Session *--> "1" UniqueQuestionList
UniqueQuestionList o--> "*" Question
Question *--> QuestionName
Question *--> Number
Question *--> Answer

Session *--> "1" UniqueMetricList
UniqueMetricList o--> "*" Metric

@enduml