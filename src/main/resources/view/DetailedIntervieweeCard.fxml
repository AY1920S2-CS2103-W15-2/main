<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="detailedIntervieweePane" maxWidth="250" minHeight="490" minWidth="250" prefHeight="490" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="name" layoutX="20.0" layoutY="10.0" styleClass="cell_big_label" text="\$first" />
     <Label fx:id="alias" styleClass="cell_small_label" text="\$alias" />
    <Label fx:id="id" layoutX="20.0" layoutY="35.0" styleClass="cell_small_label" text="\$id" />
     <Button fx:id="viewResume" alignment="CENTER_RIGHT" mnemonicParsing="false" prefHeight="25.0" textAlignment="CENTER" />
     <BarChart fx:id="scoreChart">
         <xAxis>
             <NumberAxis fx:id="xAxis" side="BOTTOM" />
         </xAxis>
         <yAxis>
             <CategoryAxis side="LEFT" fx:id="yAxis" />
         </yAxis>
     </BarChart>
</VBox>
