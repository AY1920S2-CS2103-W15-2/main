<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <BorderPane prefHeight="87.0" prefWidth="200.0">
      <top>
         <HBox prefHeight="23.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
               <Label fx:id="metricName" text="\$metric name" />
            </children>
         </HBox>
      </top>
      <center>
         <TableView fx:id="attributeToWeight" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
           <columns>
             <TableColumn prefWidth="75.0" text="Attribute" />
             <TableColumn prefWidth="75.0" text="Weight" />
           </columns>
         </TableView>
      </center>
   </BorderPane>
</HBox>
