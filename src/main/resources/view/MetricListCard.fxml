<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.layout.VBox?>
   <BorderPane prefHeight="87.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
      <top>
         <HBox prefHeight="23.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
               <Label fx:id="metricName" text="\$metric name" />
            </children>
         </HBox>
      </top>
      <center>
          <VBox>
             <TableView fx:id="attributeToWeight" >
               <columns>
                 <TableColumn text="Attribute" />
                 <TableColumn text="Weight" />
               </columns>
             </TableView>
          </VBox>
      </center>
   </BorderPane>

